# Скопируйте этот файл как .env и заполните значения
TELEGRAM_BOT_TOKEN=1234567890:ABCDEF_your_token_here
TELEGRAM_CHAT_ID=-1001234567890
# Интервал опроса API в секундах (по умолчанию 30)
POLL_INTERVAL=30
# Сколько последних рынков проверять на каждом цикле
FETCH_LIMIT=50
# Отправлять ли оповещения обо ВСЕХ найденных рынках при первом запуске (true/false)
NOTIFY_ON_START=false
